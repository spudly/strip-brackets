{
  "name": "strip-brackets",
  "private": true,
  "devDependencies": {
    "@spudly/eslint-config": "^5.1.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^7.0.1",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.0"
  },
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "build": "babel src -d build"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4,
            "browsers": "> 1%"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@spudly",
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write --print-width=100 --single-quote --bracket-spacing=false --trailing-comma=all",
      "git add"
    ]
  },
  "files": ["build"]
}
